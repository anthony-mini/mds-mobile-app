{
  "expo": {
    "name": "mds-mobile-app",
    "slug": "mds-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#514EB7"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "font": [
            "./assets/fonts/ClashDisplay-Bold.otf",
            "./assets/fonts/ClashDisplay-Light.otf",
            "./assets/fonts/ClashDisplay-Regular.otf",
            "./assets/fonts/ClashDisplay-Extralight.otf",
            "./assets/fonts/ClashDisplay-Medium.otf",
            "./assets/fonts/ClashDisplay-Semibold.otf",
            "./assets/fonts/ClashGrotesk-Bold.otf",
            "./assets/fonts/ClashGrotesk-Light.otf",
            "./assets/fonts/ClashGrotesk-Regular.otf",
            "./assets/fonts/ClashGrotesk-Extralight.otf",
            "./assets/fonts/ClashGrotesk-Medium.otf",
            "./assets/fonts/ClashGrotesk-Semibold.otf"
          ]
        }
      ]
    ]
  }
}
